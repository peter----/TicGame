<Window x:Class="TicTacToeWPF.PlayerName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Player Names" Height="150" Width="300">
    <StackPanel Margin="10">
        <TextBox x:Name="TxtX" Margin="0,0,0,5" Text="Player X"/>
        <TextBox x:Name="TxtO" Margin="0,0,0,5" Text="Player O"/>
        <Button Content="Start" Click="Start_Click"/>
    </StackPanel>
</Window>